  compatible = "sifive,fu540-c000", "sifive,fu540", "simple-bus";
  ranges;
  plic0: interrupt-controller@c000000 {
   #interrupt-cells = <1>;
   compatible = "sifive,plic-1.0.0";
   reg = <0x0 0xc000000 0x0 0x4000000>;
   riscv,ndev = <53>;
