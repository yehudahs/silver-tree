   interrupts = <4>;
   clocks = <&prci 3>;
   status = "disabled";
  };
  dma: dma@3000000 {
   compatible = "sifive,fu540-c000-pdma";
   reg = <0x0 0x3000000 0x0 0x8000>;
   interrupt-parent = <&plic0>;
   interrupts = <23 24 25 26 27 28 29 30>;
   #dma-cells = <1>;
  };
  uart1: serial@10011000 {
